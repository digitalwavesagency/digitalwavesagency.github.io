        {
          "_name": "header2",
          "_sourceTheme": "edgem5",
          "_customHTML": "<section class=\"header2\" group=\"Contact\" data-bg-video=\"{{bg.type == 'video' && bg.value.url}}\" mbr-class=\"{\n    'mbr-fullscreen': fullScreen,\n    'mbr-parallax-background': bg.parallax}\">\n\n    <mbr-parameters>\n        <header>Size</header>\n        <input type=\"checkbox\" title=\"Full Screen\" name=\"fullScreen\" checked>\n        <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\" checked>\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"10\" condition=\"fullScreen == false\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"10\" condition=\"fullScreen == false\">\n\n        <input type=\"range\" inline name=\"textWidth\" title=\"Width\" min=\"4\" max=\"12\" value=\"12\" step=\"1\">\n        <header>Show/Hide</header>\n        <input type=\"checkbox\" title=\"Text\" name=\"showLabel\" checked>\n        <input type=\"checkbox\" title=\"Title\" name=\"showTitle\" checked>\n        <input type=\"checkbox\" title=\"Buttons\" name=\"showButtons\" checked>\n        <select title=\"Advantages\" name=\"cardsAmount\">\n            <option value=\"0\">0</option>\n            <option value=\"1\">1</option>\n            <option value=\"2\">2</option>\n            <option value=\"3\" selected>3</option>\n        </select>\n        <input type=\"checkbox\" title=\"Icon\" name=\"showIcon\" condition=\"cardsAmount > 0\" checked=\"\">\n        <input type=\"color\" title=\"Icon Color\" name=\"iconColor\" value=\"#002549\" condition=\"cardsAmount > 0 &amp;&amp; showIcon\">\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\" parallax>\n            <input type=\"image\" title=\"Image\" value=\"@ADDONS_PATH@/edgem5/components/_images/background7.svg\">\n            <input type=\"color\" title=\"Color\" value=\"#2e71b8\">\n            <input type=\"video\" title=\"Video\" value=\"https://vimeo.com/106601778\" selected>\n        </fieldset>\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked>\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#2e71b8\" condition=\"overlay && bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.3\" condition=\"overlay && bg.type !== 'color'\">\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type !== 'color'\" opacity=\"{{overlayOpacity}}\" bg-color=\"{{overlayColor}}\"></div>\n\n    <div class=\"align-center\" mbr-class=\"{'container': !fullWidth, 'container-fluid': fullWidth}\">\n        <div class=\"row justify-content-center\">\n            <div class=\"col-12 col-text col-lg-{{textWidth}}\">\n                <p class=\"mbr-text label-text mbr-fonts-style mb-0\" mbr-theme-style=\"display-4\" mbr-if=\"showLabel\" data-app-selector=\".label-text\">\n                    <b>CONTACT</b>\n                </p>\n                <h1 class=\"mbr-section-title mbr-fonts-style mb-0\" data-app-selector=\".mbr-section-title, .mbr-section-btn\" mbr-theme-style=\"display-1\" mbr-if=\"showTitle\"><b>\n                    Let's talk\n                </b></h1>\n                <div mbr-buttons mbr-theme-style=\"display-5\" class=\"mbr-section-btn mt-3\" mbr-if=\"showButtons\"><a class=\"btn btn-white-outline\" href=\"mailto:hello@digitalwaves.agency\" data-app-placeholder=\"Type Text\">Book a call</a></div>\n                <div class=\"advantages-container\" mbr-if=\"cardsAmount > 0\">\n                    <div class=\"advantage\" mbr-if=\"cardsAmount > 0\">\n                        <div class=\"advantage-icon\" mbr-if=\"showIcon\">\n                            <span mbr-icon class=\"mbr-iconfont mobi-mbri-success mobi-mbri\" style=\"font-size: 10px;\"></span>\n                        </div>\n                        <p class=\"mbr-text advantage-text mbr-fonts-style mb-0\" mbr-theme-style=\"display-7\" data-app-selector=\".advantage-text\">\n                            Tailored</p>\n                    </div>\n                    <div class=\"advantage\" mbr-if=\"cardsAmount > 1\">\n                        <div class=\"advantage-icon\" mbr-if=\"showIcon\">\n                            <span mbr-icon class=\"mbr-iconfont mobi-mbri-success mobi-mbri\" style=\"font-size: 10px;\"></span>\n                        </div>\n                        <p class=\"mbr-text advantage-text mbr-fonts-style mb-0\" mbr-theme-style=\"display-7\" data-app-selector=\".advantage-text\">\n                            Full Service</p>\n                    </div>\n                    <div class=\"advantage\" mbr-if=\"cardsAmount > 2\">\n                        <div class=\"advantage-icon\" mbr-if=\"showIcon\">\n                            <span mbr-icon class=\"mbr-iconfont mobi-mbri-success mobi-mbri\" style=\"font-size: 10px;\"></span>\n                        </div>\n                        <p class=\"mbr-text advantage-text mbr-fonts-style mb-0\" mbr-theme-style=\"display-7\" data-app-selector=\".advantage-text\">\n                            Out of the Box</p>\n                    </div>\n                </div>\n            </div>\n        </div>\n    </div>\n</section>",
